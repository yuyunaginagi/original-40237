<div class="contents row">
  <% @results.each do |result| %>
      <p><%= result.date %></p>
      <p><%= result.game_name %></p>
      <span class="name">
        <%= result.event.name %>
        <%= result.result %>
              <div class="more">
        <ul class="more_list">
          <li>
            <%= link_to '削除', result_path(result.id), data: { turbo_method: :delete } %>
          </li>
        </ul>
      </div>
      </span>
  <% end %>
</div>